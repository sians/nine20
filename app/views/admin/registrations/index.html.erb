<h1>Registered Users</h1>
<% @users.each do |user| %>
  <div class="user-card">
    <div class="user-details">
      <p><%= user.first_name %> <%= user.last_name %></p>
      <%= user.email %>
    </div>
    <div class="buttons">
    </div>
  </div>
<% end %>

<%= link_to 'sign out', destroy_user_session_path, method: :delete %>

<%# raise %>
